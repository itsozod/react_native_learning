{
  "expo": {
    "name": "Frontend Quiz",
    "slug": "react_native_learning",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/quiz_front.jpg",
    "scheme": "reactnativelearning",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/shared/assets/images/quiz_front.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ozod2905.react-native-learning",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/quiz_front.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ozod2905.react_native_learning",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/shared/assets/images/quiz_front.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ac2725b7-aae6-4666-a452-8f18544401cd"
      }
    },
    "owner": "ozod2905"
  }
}
